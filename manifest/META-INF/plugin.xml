<idea-plugin version="2">
  <id>org.osmorc.manifest</id>
  <name>Manifest support</name>
  <version>1.0</version>
  <idea-version since-build="129.000"/>

  <extensionPoints>
    <extensionPoint name="headerParser" beanClass="org.osmorc.manifest.lang.headerparser.HeaderParserEP">
      <with attribute="implementationClass" implements="org.osmorc.manifest.lang.headerparser.HeaderParser"/>
    </extensionPoint>
  </extensionPoints>

  <extensions defaultExtensionNs="org.osmorc.manifest">
    <headerParser key="" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser" order="last"/>

    <headerParser key="Created-By" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Signature-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Class-Path" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Implementation-Title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Implementation-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Implementation-Vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Implementation-Vendor-Id" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Implementation-URL" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Specification-Title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Specification-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Specification-Vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Sealed" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Content-Type" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Java-Bean" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="MD5-Digest" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="SHA-Digest" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Magic" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Name" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Manifest-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>

    <!--TODO [VISTALL] this is list of futures - that need replace to other plugins-->
    <!--[JavaFX] JavaFX futures-->
    <headerParser key="JavaFX-Application-Class"
                  implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="JavaFX-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="JavaFX-Class-Path" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Main-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="implementation-vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="implementation-title" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="implementation-version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="JavaFX-Preloader-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="JavaFX-Fallback-Class" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>

    <!--[OSGi] Bnd futures-->
    <headerParser key="Private-Package" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Tool" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Bnd-LastModified" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Include-Resource" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>

    <!--[SpringDM] Spring futures-->
    <headerParser key="Spring-Context" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Import-Library" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Library-SymbolicName" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Library-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Library-Name" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Library-Description" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Import-Bundle" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Application-TraceLevels"
                  implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="SpringExtender-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Module-Type" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Web-ContextPath" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Web-DispatcherServletUrlPatterns"
                  implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>

    <!--[OSGi] Equinox futures-->
    <headerParser key="Eclipse-PlatformFilter" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Eclipse-BuddyPolicy" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Eclipse-RegisterBuddy" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Eclipse-ExtensibleAPI" implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Eclipse-GenericCapability"
                  implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Eclipse-GenericRequire" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>

    <!--[OSGi] OSGi futures-->
    <headerParser key="Bundle-ManifestVersion" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Name" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-SymbolicName" implementationClass="org.osmorc.manifest.lang.headerparser.impl.BundleSymbolicNameParser"/>
    <headerParser key="Bundle-Version" implementationClass="org.osmorc.manifest.lang.headerparser.impl.BundleVersionParser"/>
    <headerParser key="Export-Package" implementationClass="org.osmorc.manifest.lang.headerparser.impl.ExportPackageParser"/>
    <headerParser key="Import-Package" implementationClass="org.osmorc.manifest.lang.headerparser.impl.ImportPackageParser"/>
    <headerParser key="Require-Bundle" implementationClass="org.osmorc.manifest.lang.headerparser.impl.RequireBundleParser"/>
    <headerParser key="Bundle-RequiredExecutionEnvironment"
                  implementationClass="org.osmorc.manifest.lang.headerparser.impl.GenericComplexHeaderParser"/>
    <headerParser key="Fragment-Host" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-ActivationPolicy" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Activator" implementationClass="org.osmorc.manifest.lang.headerparser.impl.BundleActivatorParser"/>
    <headerParser key="Bundle-Category" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-ClassPath" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-ContactAddress" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Copyright" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Description" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-DocURL" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Localization" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-NativeCode" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-UpdateLocation" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Bundle-Vendor" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="DynamicImport-Package" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Export-Service" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Import-Service" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
    <headerParser key="Service-Component" implementationClass="org.osmorc.manifest.lang.headerparser.impl.SimpleHeaderParser"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij">
    <colorSettingsPage implementation="org.osmorc.manifest.lang.ManifestColorsAndFontsPage"/>
    <errorHandler implementation="com.intellij.diagnostic.ITNReporter"/>
    <lang.parserDefinition language="Manifest" implementationClass="org.osmorc.manifest.lang.ManifestParserDefinition"/>
    <completion.contributor language="Manifest" implementationClass="org.osmorc.manifest.ManifestCompletionContributor"/>
    <syntaxHighlighter key="Manifest" factoryClass="org.osmorc.manifest.lang.ManifestSyntaxHighlighterProvider"/>
    <annotator language="Manifest" implementationClass="org.osmorc.manifest.lang.headerparser.HeaderAnnotator"/>
    <annotator language="Manifest" implementationClass="org.osmorc.manifest.lang.ManifestHighlightingAnnotator"/>
    <lang.elementManipulator forClass="org.osmorc.manifest.lang.psi.HeaderValuePart"
                             implementationClass="org.osmorc.manifest.lang.psi.HeaderValuePartManipulator"/>
    <lang.ast.factory language="Manifest" implementationClass="org.osmorc.manifest.lang.psi.impl.ManifestASTFactory"/>
  </extensions>
</idea-plugin>